<section id="hero-section">
  <div class="container">
    {% if isLogo %}
      <img class="logo" src="{{ "logo-new.svg" | asset_url }}" width='264'  height="61" alt="logo">
    {% endif %}

    <h1>Natural &<br>Effective Blood<br>Sugar Support</h1>
    <div class="brand-welcome-text">
      <h2 class="v3">So you can
        <span class="title-text">eat well</span>
        <span class="title-text" style="display: none;">sleep well</span>
        <span class="title-text" style="display: none;">feel energetic</span>
        <span class="title-text" style="display: none;">stay romantic</span>
        <span class="title-text" style="display: none;">stay healthy</span>
      </h2>
    </div>
    <div class="brand-intro-cover">
      <span class="brand-intro-cover-img">
        <img class="cover-img" src="{{ 'lp-aio-v3-hero-img-1.png' | file_url}}" alt="">
        <img class="cover-img" src="{{ 'lp-aio-v3-hero-img-2.png' | file_url}}" alt="" style="display: none;">
        <img class="cover-img" src="{{ 'lp-aio-v3-hero-img-3.png' | file_url}}" alt="" style="display: none;">
        <img class="cover-img" src="{{ 'lp-aio-v3-hero-img-4.png' | file_url}}" alt="" style="display: none;">
        <img class="cover-img" src="{{ 'lp-aio-v3-hero-img-5.png' | file_url}}" alt="" style="display: none;">
      </span>
      <span class="brand-intro-cover-img-mobile">
        <img class="cover-img-mobile" src="{{ 'lp-aio-v3-hero-img-1-mobile.png' | file_url}}" alt="">
        <img class="cover-img-mobile" src="{{ 'lp-aio-v3-hero-img-2-mobile.png' | file_url}}" alt="" style="display: none;">
        <img class="cover-img-mobile" src="{{ 'lp-aio-v3-hero-img-3-mobile.png' | file_url}}" alt="" style="display: none;">
        <img class="cover-img-mobile" src="{{ 'lp-aio-v3-hero-img-4-mobile.png' | file_url}}" alt="" style="display: none;">
        <img class="cover-img-mobile" src="{{ 'lp-aio-v3-hero-img-5-mobile.png' | file_url}}" alt="" style="display: none;">
      </span>
    </div>
  </div>
</section>
<!-- Hero Section from General Page Style -->
<style>

  *,
  *:before,
  *:after {
  -webkit-box-sizing: border-box;
  box-sizing: border-box
  }
  body {
    margin: 0;
  }

   .container {
   width: 100%;
   max-width: 1470px;
   margin: 0 auto;
   padding: 0 15px
   }

  #hero-section {
    padding: 50px 0;
  }

  #hero-section:before {
    content: "";
    width: 100vw;
    position: absolute;
    top: -30px;
    height: 510px;
    left: 0;
    z-index: -5;
    background: #e8e8e8;
  }

  #hero-section img.logo {
    width: 150px;
    margin-bottom: 50px;
  }

  #hero-section .brand-intro-cover span {
    width: 80%;
    display: block;
    position: absolute;
    top: -6.625rem;
    right: -4.875rem;
    bottom: 0;
    z-index: -1
  }

  #hero-section .brand-intro-cover span img {
    width: 100%;
    height: 100%;
    -o-object-fit: contain;
    object-fit: contain
  }

  #hero-section h1 {
    font-size: 2.7rem;
    color: #000000ab
  }

  #hero-section .brand-welcome-text {
    padding: 2rem 0 10.75rem;
    text-align: left;

  }

  #hero-section .brand-welcome-text * {
    font-weight: 400;
    font-size: 5rem;
    line-height: 1.2;
    letter-spacing: -.03em;
    color: #fff;
    margin: auto
  }

  #hero-section .brand-welcome-text span,
  #hero-section .brand-welcome-text b,
  #hero-section .brand-welcome-text strong {
    font-weight: 700
  }

  #hero-section .brand-welcome-text span.typed-cursor {
    font-weight: 200
  }

  @media screen and (max-width: 991px) {
    #hero-section .brand-logo {
      max-width: 9.5rem
    }

    #hero-section:before {
      bottom: 2.5rem
    }

    #hero-section .brand-welcome-text * {
      font-size: 3.125rem
    }

    #hero-section .brand-welcome-text {
      padding: 4.375rem 0 2.5rem
    }

    #hero-section .brand-intro-cover span {
      top: -1.25rem
    }

    @media screen and (max-width: 768px) {

      #hero-section .brand-intro-cover span {
        width: 100%;
        position: static
      }

      #hero-section .brand-intro-cover {
        margin: 0 -6.0625rem 0 -3.25rem;
      }

      #hero-section h1 {
        text-align: center;
        font-size: 1.75rem;
      }

      #hero-section .brand-welcome-text * {
        font-size: 7vw;
        color: var(--green);
      }

      #hero-section .brand-welcome-text {
        padding: .875rem 0;
        margin: 0 -15px 0 5%;
      }

      #hero-section:before {
        bottom: 0;
      }
    }
  }
</style>

<!-- Hero Section Snippet Changes Style -->
<style>
  #hero-section {

  }
  .brand-intro-cover-img-mobile {
    display: none !important;
  }

  #hero-section .brand-welcome-text {
    padding: 10rem 0 3.5rem;
  }

  #hero-section .brand-welcome-text * {
    color: white;
    line-height: 1;
    font-size: clamp(6vw, 4vw, 80px) !important;
  }

  #hero-section .brand-welcome-text h2 {
    background: var(--green);
    padding: 0 30px;
    width: max-content;

  }

  @media screen and (max-width: 768px) {
    .brand-logo {
      margin: auto;
      margin-top: 10px;
    }

    .brand-intro-cover-img-mobile {
      display: block !important;
      position: absolute !important;
      top: 190px !important;
      height: 80vw;
      width: 110vw !important;
      left: -8vw;
    }

    #hero-section .brand-welcome-text * {
      font-size: 6vw;
      color: {% if regColor %}#62C1C5 {% else %} #4B643D {% endif %};
      background: unset !important;
      padding: 0 !important;
    }

    #hero-section .brand-welcome-text {
      margin: 0;
      padding-top: 10px;
      margin-left: 6vw;
    }

    .brand-intro-cover-img {
      display: none !important;
    }

    #hero-section {
      min-height: 150vw;
      overflow: visible;
    }

    #hero-section:before {
      min-height: 140vw;
    }
  }

</style>

<script>
  // Fade Quotes
  var quotes = $(".title-text");
  var quoteIndex = -1;

  function showNextQuote() {
    ++quoteIndex;
    quotes.eq(quoteIndex % quotes.length)
      .fadeIn(900)
      .delay(1500)
      .fadeOut(900, showNextQuote);
  }

  showNextQuote();

  $next = 1;
  $current = 0;
  $nextM = 1;
  $currentM = 0;
  $interval = 1500;
  $fadeTime = 1800;
  $imgNum = 5;

  $(document).ready(function () {
    $('.brand-intro-cover-img img').css({ 'position': 'absolute' });
    $('.brand-intro-cover-img-mobile img').css({ 'position': 'absolute' });
    nextFadeIn();
    nextFadeInM();
  });

  function nextFadeIn() {
    $('.brand-intro-cover-img img').eq($current).delay($interval).fadeOut($fadeTime)
      .end().eq($next).delay($interval).hide().fadeIn($fadeTime, nextFadeIn);

    if ($next < $imgNum - 1) { $next++; } else { $next = 0; }
    if ($current < $imgNum - 1) { $current++; } else { $current = 0; }
  };

  function nextFadeInM() {
    $('.brand-intro-cover-img-mobile img').eq($currentM).delay($interval).fadeOut($fadeTime)
      .end().eq($nextM).delay($interval).hide().fadeIn($fadeTime, nextFadeInM);

    if ($nextM < $imgNum - 1) { $nextM++; } else { $nextM = 0; }
    if ($currentM < $imgNum - 1) { $currentM++; } else { $currentM = 0; }
  };
</script>

<script>
  if (!$("#hero-section .brand-welcome-text .v3").length) {
    let typed = new Typed('#hero-section .brand-welcome-text span', {
      strings: ["sleep well", "eat well", "feel energetic", "stay romantic", "stay healthy"],
      typeSpeed: 50,
      backDelay: 3000,
      backSpeed: 30,
      loop: true
    });
  }
</script>