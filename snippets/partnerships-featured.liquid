<style>


.container {
  width: 100%;
  max-width: 1470px;
  margin: 0 auto;
  padding: 0 15px
}


  .featured-section {
    font-family: "Poppins", sans-serif;
    padding-top: 1.0625rem;
    padding-bottom: 3.6875rem
  }

  .featured-section .sub-title {
    text-align: center
  }

  .featured-section .sub-title h4 {
    font-size: 1.5rem;
    letter-spacing: -.03em;
    opacity: .6
  }

  .partners-wrapper {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    justify-content: center;
    padding-top: 1.1875rem
  }

  .partners-wrapper .ticker-wrap {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    margin: 0 -.9375rem;
    padding: 0 .9375rem;
    position: relative;
    z-index: 1;
    overflow: hidden
  }

  .partners-wrapper .ticker-wrap:before,
  .partners-wrapper .ticker-wrap:after {
    content: "";
    width: .9375rem;
    position: absolute;
    top: 0;
    bottom: 0;
    z-index: 3;
    pointer-events: none
  }

  .partners-wrapper .ticker-wrap:before {
    left: 0;
    background: -webkit-gradient(linear, left top, right top, from(#F7F7F7), color-stop(49.3%, rgba(255, 255, 255, .76)), to(rgba(255, 255, 255, 0)));
    background: -o-linear-gradient(left, #F7F7F7 0%, rgba(255, 255, 255, .76) 49.3%, rgba(255, 255, 255, 0) 100%);
    background: linear-gradient(90deg, #F7F7F7 0%, rgba(255, 255, 255, .76) 49.3%, rgba(255, 255, 255, 0) 100%)
  }

  .partners-wrapper .ticker-wrap:after {
    right: 0;
    background: -webkit-gradient(linear, left top, right top, from(rgba(255, 255, 255, 0)), color-stop(49.3%, rgba(255, 255, 255, .76)), to(#F7F7F7));
    background: -o-linear-gradient(left, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, .76) 49.3%, #F7F7F7 100%);
    background: linear-gradient(90deg, rgba(255, 255, 255, 0) 0%, rgba(255, 255, 255, .76) 49.3%, #F7F7F7 100%)
  }

  .partners-wrapper .ticker-wrap .ticker-col {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -ms-flex-negative: 0;
    flex-shrink: 0;
    transition-timing-function: linear
  }

  .partners-wrapper .partner-col {
    max-width: -webkit-max-content;
    max-width: -moz-max-content;
    max-width: max-content;
    padding-left: 1.875rem;
    padding-right: 1.875rem
  }

  .partners-wrapper .partner-item {
    opacity: .4
  }

  @-webkit-keyframes ticker {
    to {
      -webkit-transform: translateX(-100%);
      transform: translate(-100%)
    }
  }

  @keyframes ticker {
    to {
      -webkit-transform: translateX(-100%);
      transform: translate(-100%)
    }
  }


  @media screen and (min-width: 992px) {
    .partners-wrapper .ticker-wrap .ticker-col {
      display: none;
      -webkit-animation: none;
      animation: none
    }

    .partners-wrapper .ticker-wrap .ticker-col:first-child {
      display: -webkit-box;
      display: -ms-flexbox;
      display: flex
    }

    @media screen and (max-width: 991px) {
      .partners-wrapper .partner-col {
        padding-left: 1rem;
        padding-right: 1rem
      }

      .featured-section {
        padding-top: 1.875rem;
        padding-bottom: 1.875rem
      }
  
    }

    @media screen and (max-width: 760px) {
      .featured-section .sub-title h4 {
        font-size: 1.125rem;
        letter-spacing: normal
      }

      .featured-section {
        padding-top: 1.75rem;
        padding-bottom: 1.75rem
      }

      .partners-wrapper {
        margin: 0 -15px
      }

      .partners-wrapper .ticker-wrap {
        margin: 0;
        padding: 0
