<div class="companies-logo">
        <div class="container">
            <div class="logo-list wow fadeInUp">
              {% for block in section.blocks %}
                <span><img class="lazyload" data-src="{{ block.settings.assets | img_url : 'master' }}" alt=""></span>
              {% endfor %}
            </div>
        </div>
    </div>


{% schema %}
    {
    "name": "Companies Logo",
    "blocks": [
    {
    "type": "team",
    "name": "Logo",
    "settings": [
    {
    "type": "image_picker",
    "id": "assets",
    "label": "Image"
    }
    ]
    }
    ]
    }
{% endschema %}