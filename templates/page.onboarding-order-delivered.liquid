<style>
  @font-face {
    font-family: 'DM Sans';
    font-style: normal;
    font-weight: 400;
    src: url('{{ "DMSans-Regular.ttf" | asset_url }}');
    src: local(''), url('{{ "DMSans-Regular.ttf" | asset_url }}') format('ttf'),
  }

  @font-face {
    font-family: 'DM Sans';
    font-style: normal;
    font-weight: 500;
    src: url('{{ "DMSans-Medium.ttf" | asset_url }}');
    src: local(''), url('{{ "DMSans-Medium.ttf" | asset_url }}') format('ttf'),
  }

  @font-face {
    font-family: 'DM Sans';
    font-style: normal;
    font-weight: 700;
    src: url('{{ "DMSans-Bold.ttf" | asset_url }}');
    src: local(''), url('{{ "DMSans-Bold.ttf" | asset_url }}') format('ttf'),
  }

  @font-face {
    font-family: 'PP Radio Grotesk';
    font-style: normal;
    font-weight: 300;
    src: url('{{ "PPRadioGrotesk-Light.otf" | asset_url }}');
    src: local(''), url('{{ "PPRadioGrotesk-Light.otf" | asset_url }}') format('otf'), }

  @font-face {
    font-family: 'PP Radio Grotesk';
    font-style: normal;
    font-weight: 400;
    src: url('{{ "PPRadioGrotesk-Regular.otf" | asset_url }}');
    src: local(''), url('{{ "PPRadioGrotesk-Regular.otf" | asset_url }}') format('otf'), }

  @font-face {
    font-family: 'PP Radio Grotesk';
    font-style: normal;
    font-weight: 700;
    src: url('{{ "PPRadioGrotesk-Bold.otf" | asset_url }}');
    src: local(''), url('{{ "PPRadioGrotesk-Bold.otf" | asset_url }}') format('otf'), }
</style>

{% capture CFH %}{{ content_for_header }}{% endcapture %}{{CFH}}

{% if CFH contains "started=true" %}
  {% assign progress_steps =  "How To Use
                              |Reviews
                              |Support
                              |FAQ"
                              | split: '|' %}

  {% render 'onboarding-hero', title: "Congrats on starting your journey with Curalin!", icon_file: "onboarding-applause-icon.png" %}
  {% render 'onboarding-progress-bar', progress_steps: progress_steps %}
  {% render 'onboarding-curalin-consumers-started' %}
  {% render 'onboarding-curalin-best-results' %}
  {% render 'onboarding-curalin-call-expert', background_color: '#FAF5E9', border: false %}
  {% render 'onboarding-faq' %}
{% else %}
  {% assign progress_steps =  "Support
                                |How To Use
                                |Reviews
                                |FAQ"
                                | split: '|' %}

  {% render 'onboarding-hero', title: "Curalin has been delivered - lets start!", icon_file: "onboarding-deliver-icon.png"%}
  {% render 'onboarding-progress-bar', progress_steps: progress_steps %}
  {% render 'onboarding-curalin-call-expert', background_color: 'unset', border: true %}
  {% render 'onboarding-curalin-consumers-not-started'%}
  {% render 'onboarding-faq' %}
{% endif %}